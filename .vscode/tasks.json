{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Check Git remotes",
			"type": "shell",
			"command": "git",
			"args": [
				"remote",
				"-v"
			],
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "Git Add All",
			"type": "shell",
			"command": "git",
			"args": [
				"add",
				"-A"
			],
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}